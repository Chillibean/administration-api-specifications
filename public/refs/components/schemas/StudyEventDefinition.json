{
  "description": "A study event definition",
  "type": "object",
  "properties": {
    "display_name": {
      "type": "string",
      "description": "The display name of study event definition",
      "example": "Day 1"
    },
    "id": {
      "type": "number",
      "description": "The id of study event definition",
      "example": 1
    },
    "identifier": {
      "type": "string",
      "description": "The identifier of study event definition",
      "example": "DAY1"
    },
    "position": {
      "type": "number",
      "description": "The position of study event definition",
      "example": 1
    },
    "study_arm": {
      "anyOf": [
        {
          "$ref": "./StudyArm.json"
        },
        {
          "type": "object",
          "nullable": true
        }
      ]
    },
    "study_events_exist": {
      "type": "boolean",
      "description": "The indicator whether study events exist",
      "example": false
    }
  },
  "additionalProperties": false,
  "required": [
    "display_name",
    "id",
    "identifier",
    "position",
    "study_events_exist"
  ]
}
