{
  "description": "A site",
  "type": "object",
  "properties": {
    "country": {
      "type": "string",
      "description": "The country of site",
      "example": "United Kingdom"
    },
    "go_live_date": {
      "type": "string",
      "description": "The go live date of site",
      "example": "2024-06-10"
    },
    "id": {
      "type": "number",
      "description": "The id of site",
      "example": 7
    },
    "identifier": {
      "type": "string",
      "description": "The identifier of site",
      "example": "BEACONHILL"
    },
    "name": {
      "type": "string",
      "description": "The name of site",
      "example": "Beacon Hill Medical Center"
    },
    "trial_configuration": {
      "type": "object",
      "description": "The trial configuration of site",
      "properties": {
        "id": {
          "type": "number",
          "description": "The id of trial configuration",
          "example": 2
        },
        "locked": {
          "type": "boolean",
          "description": "The locked status of trial configuration",
          "example": true
        },
        "name": {
          "type": "string",
          "description": "The name of trial configuration",
          "example": "Alpha Trial Configuration"
        }
      },
      "nullable": true
    }
  }
}