{
  "description": "A form template",
  "type": "object",
  "properties": {
    "active": {
      "type": "boolean",
      "description": "The active status of form template",
      "example": true
    },
    "created_at": {
      "type": "string",
      "description": "The created at of form template",
      "example": "2020-01-01T00:00:00.000Z"
    },
    "description": {
      "type": "string",
      "description": "The description of form template",
      "example": "Test"
    },
    "id": {
      "type": "number",
      "description": "The id of form template",
      "example": 1
    },
    "number_of_forms": {
      "type": "number",
      "description": "The number of forms of form template",
      "example": 1
    },
    "title": {
      "type": "string",
      "description": "The title of form template",
      "example": "Test"
    },
    "trial_containers": {
      "type": "array",
      "description": "The trial containers of form template",
      "items": {
        "$ref": "./TrialContainer.json"
      }
    },
    "version_number": {
      "type": "number",
      "description": "The version number of form template",
      "example": 1
    },
    "versions": {
      "type": "array",
      "items": {
        "$ref": "./FormTemplateVersion.json"
      }
    },
    "workflow_definition_id": {
      "type": "number",
      "description": "The workflow definition id of form template",
      "example": 1
    },
    "workflow_definition_name": {
      "type": "string",
      "description": "The workflow definition name of form template",
      "example": "Test"
    }
  }
}