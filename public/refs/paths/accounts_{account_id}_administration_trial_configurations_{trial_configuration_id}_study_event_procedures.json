{
  "post": {
    "summary": "Create a study event procedure for trial configuration",
    "description": "Create a study event procedure for trial configuration",
    "operationId": "createStudyEventProcedure",
    "tags": [
      "Trial Configurations"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      },
      {
        "name": "trial_configuration_id",
        "in": "path",
        "description": "The trial configuration id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "description": "study event procedure object",
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "description": "study event procedure object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "study_event_definition_id": {
                "type": "string",
                "description": "The study event definition id of study event procedure",
                "example": "1"
              },
              "study_procedure_definition_id": {
                "type": "string",
                "description": "The study procedure definition id of study event procedure",
                "example": "1"
              }
            },
            "additionalProperties": false,
            "required": [
              "authenticity_token",
              "study_event_definition_id",
              "study_procedure_definition_id"
            ]
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Created",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "description": "Trial configuration object",
              "properties": {
                "data": {
                  "$ref": "../components/schemas/TrialConfiguration.json"
                },
                "id": {
                  "type": "integer",
                  "description": "The id of study event procedure",
                  "example": 1
                }
              }
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/Conflict.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  }
}
