{
  "get": {
    "summary": "Get all form templates for account",
    "description": "Get all form templates for account",
    "operationId": "getFormTemplates",
    "tags": [
      "Form Templates"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "OK",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "default_form_template": {
                  "type": "object",
                  "description": "Default form template",
                  "properties": {
                    "active": {
                      "type": "boolean",
                      "description": "The active status of default form template",
                      "example": true
                    },
                    "trial_containers": {
                      "type": "array",
                      "items": {
                        "$ref": "../components/schemas/TrialContainer.json"
                      }
                    },
                    "versions": {
                      "type": "array",
                      "items": {
                        "$ref": "../components/schemas/FormTemplateVersion.json"
                      }
                    }
                  }
                },
                "form_templates": {
                  "type": "array",
                  "description": "Form templates",
                  "items": {
                    "$ref": "../components/schemas/FormTemplate.json"
                  }
                }
              }
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "500": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      }
    }
  },
  "post": {
    "summary": "Create a form template for account",
    "description": "Create a form template for account",
    "operationId": "createFormTemplate",
    "tags": [
      "Form Templates"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "formTemplateData": {
                "$ref": "../components/schemas/FormTemplateBodyData.json"
              }
            }
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Created",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/FormTemplate.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "500": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      }
    }
  }
}
