{
  "post": {
    "summary": "Create a study event definition for trial configuration",
    "description": "Create a study event definition for trial configuration",
    "operationId": "createStudyEventDefinition",
    "tags": [
      "Trial Configurations"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      },
      {
        "name": "trial_configuration_id",
        "in": "path",
        "description": "The trial configuration id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "description": "Study event definition object",
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "description": "Study event definition object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "display_name": {
                "type": "string",
                "description": "The display name of study event definition",
                "example": "Study Event Definition"
              },
              "identifier": {
                "type": "string",
                "description": "The identifier of study event definition",
                "example": "SPD"
              },
              "position": {
                "type": "integer",
                "description": "The position of study event definition",
                "example": 1
              },
              "study_arm_id": {
                "type": "string",
                "description": "The study arm id of study event definition",
                "example": "1"
              }
            },
            "additionalProperties": false,
            "required": [
              "authenticity_token",
              "display_name",
              "identifier"
            ]
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Created",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/TrialConfiguration.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/Conflict.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  }
}
