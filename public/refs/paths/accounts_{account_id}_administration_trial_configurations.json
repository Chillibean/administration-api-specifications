{
  "get": {
    "summary": "Get all trial configurations for account",
    "description": "Get all trial configurations for account",
    "operationId": "getTrialConfigurations",
    "tags": [
      "Trial Configurations"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "example": "1",
          "type": "string"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "OK",
        "content": {
          "application/json": {
            "schema": {
              "type": "array",
              "items": {
                "$ref": "../components/schemas/TrialConfiguration.json"
              }
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  },
  "post": {
    "summary": "Create a trial configuration for account",
    "description": "Create a trial configuration for account",
    "operationId": "createTrialConfiguration",
    "tags": [
      "Trial Configurations"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "example": "1",
          "type": "string"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "issue_date": {
                "type": "string",
                "description": "The issue date of trial configuration",
                "example": "2024-06-12"
              },
              "name": {
                "type": "string",
                "description": "The name of trial configuration",
                "example": "Ghost Trial Configuration"
              }
            }
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Created",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/TrialConfiguration.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/Conflict.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  }
}
