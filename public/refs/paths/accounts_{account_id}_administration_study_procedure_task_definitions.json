{
  "get": {
    "summary": "Get all study procedure task definitions",
    "description": "Get all study procedure task definitions",
    "operationId": "getStudyProcedureTaskDefinitions",
    "tags": [
      "Study Procedure Task Definitions"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "OK",
        "content": {
          "application/json": {
            "schema": {
              "type": "array",
              "description": "Study procedure task definitions",
              "items": {
                "$ref": "../components/schemas/StudyProcedureTaskDefinition.json"
              }
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "500": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      }
    }
  },
  "post": {
    "summary": "Create a study procedure task definition",
    "description": "Create a study procedure task definition",
    "operationId": "createStudyProcedureTaskDefinition",
    "tags": [
      "Study Procedure Task Definitions"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "description": "Create study procedure task definition object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "display_name": {
                "type": "string",
                "description": "The display name of study procedure task definition",
                "example": "Please complete something first"
              },
              "role_ids": {
                "type": "array",
                "description": "The role ids of study procedure task definition",
                "items": {
                  "type": "number",
                  "example": 1
                }
              }
            },
            "required": [
              "authenticity_token",
              "display_name"
            ],
            "additionalProperties": false
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Created",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/StudyProcedureTaskDefinition.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "500": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      }
    }
  }
}
