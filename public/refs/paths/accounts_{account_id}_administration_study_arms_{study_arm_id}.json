{
  "put": {
    "summary": "Update a study arm",
    "description": "Update a study arm",
    "operationId": "updateStudyArm",
    "tags": [
      "Study Arms"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      },
      {
        "name": "study_arm_id",
        "in": "path",
        "description": "The study arm id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "description": "Update study arm object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              },
              "display_name": {
                "type": "string",
                "description": "The display name of study arm",
                "example": "Post-Treatment Monitoring Arm"
              },
              "reason_for_change": {
                "type": "string",
                "description": "The reason for change of study arm",
                "example": "Mistake"
              }
            },
            "required": [
              "authenticity_token",
              "display_name"
            ],
            "additionalProperties": false
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "OK",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../components/schemas/StudyArmWithTrialConfigurations.json"
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "500": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      }
    }
  },
  "delete": {
    "summary": "Delete a study arm",
    "description": "Delete a study arm",
    "operationId": "deleteStudyArm",
    "tags": [
      "Study Arms"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      },
      {
        "name": "study_arm_id",
        "in": "path",
        "description": "The study arm id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "description": "Delete study arm object",
            "properties": {
              "authenticity_token": {
                "$ref": "../components/schemas/AuthenticityToken.json"
              }
            },
            "required": [
              "authenticity_token"
            ],
            "additionalProperties": false
          }
        }
      }
    },
    "responses": {
      "204": {
        "description": "No Content"
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      },
      "500": {
        "$ref": "../components/responses/LegacyBadRequest.json"
      }
    }
  }
}
